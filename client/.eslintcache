Todo todo={todo} />\n              </td>\n              <td>\n                <button\n                  className=\"btn btn-danger\"\n                  onClick={() => deleteTodo(todo.todo_id)}\n                >\n                  Delete\n                </button>\n              </td>\n            </tr>\n          ))}\n        </tbody>\n      </table>\n    </Fragment>\n  );\n};\n\nexport default ListTodos;\n","/home/seth/Desktop/Tutorials/todo-PERN/client/src/components/EditTodo.js",["27"],"/home/seth/Desktop/Tutorials/todo-PERN/client/src/index.js",[],{"ruleId":"28","severity":2,"message":"29","line":12,"column":8,"nodeType":"30","endLine":12,"endColumn":17},{"ruleId":"31","severity":1,"message":"32","line":11,"column":19,"nodeType":"33","messageId":"34","endLine":11,"endColumn":27},{"ruleId":"31","severity":1,"message":"35","line":21,"column":13,"nodeType":"33","messageId":"34","endLine":21,"endColumn":23},{"ruleId":"31","severity":1,"message":"32","line":10,"column":13,"nodeType":"33","messageId":"34","endLine":10,"endColumn":21},"react/jsx-no-undef","'ListTodos' is not defined.","JSXIdentifier","no-unused-vars","'response' is assigned a value but never used.","Identifier","unusedVar","'deleteTodo' is assigned a value but never used."]